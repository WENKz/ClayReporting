@model IEnumerable<ClayReporting.DataAcces.Modeles.DataExport>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.lot)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.id_composant)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.id_couleur)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.id_etat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.id_etat_1)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @if (item.lot != null) {
                    @Html.DisplayFor(modelItem => item.lot)
                        }
            </td>
         
        </tr>
    }
    
    </table>
</body>
</html>
